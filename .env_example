# ==================== 공통 테스트 기본값 ====================
TEST_USER_ID=TestSession
TEST_PROJECT_NAME=test
TEST_DB_NAME=neo4j
TEST_LOCALE=ko
TEST_DBMS=postgres
LLM_API_KEY=changeme

# ==================== 경로 설정 (선택 사항) ====================
# 지정하지 않으면 data/<TEST_USER_ID>/<TEST_PROJECT_NAME> 경로 사용
TEST_DATA_DIR=

# ==================== Understanding 파이프라인 옵션 ====================
# 1이면 그래프 초기화를 생략하여 누적 모드로 동작
TEST_MERGE_MODE=0
# 분석 전략 선택 (dbms / framework 등)
TEST_ANALYSIS_STRATEGY=dbms

# 한글 로그 깨짐 방지를 위해 권장
PYTHONIOENCODING=utf-8

# ==================== Neo4j 연결 설정 ====================
NEO4J_URI=bolt://127.0.0.1:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=neo4j

# ==================== LLM 설정 ====================
LLM_API_BASE=https://api.openai.com/v1
LLM_MODEL=gpt-4.1
LLM_MAX_TOKENS=32768
LLM_REASONING_EFFORT=medium

# 커스텀 LLM 사용 여부 (true/false 또는 1/0)
IS_CUSTOM_LLM=false
COMPANY_NAME=

# ==================== 성능 튜닝 옵션 ====================
# DDL 처리 최대 동시성
DDL_MAX_CONCURRENCY=5

# 변수 처리 최대 동시성
VARIABLE_CONCURRENCY=5

# 일반 최대 동시성
MAX_CONCURRENCY=5
